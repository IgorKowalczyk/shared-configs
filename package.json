{
  "name": "@igorkowalczyk/sharable-configs",
  "version": "3.0.3",
  "description": "â˜” My sharable ESLint and Prettier configs",
  "author": "Igor Kowalczyk",
  "license": "MIT",
  "repository": "IgorKowalczyk/shared-configs",
  "funding": "https://github.com/sponsors/igorkowalczyk",
  "bugs": "https://github.com/IgorKowalczyk/shared-configs/issues",
  "homepage": "https://github.com/IgorKowalczyk/shared-configs#readme",
  "private": true,
  "type": "module",
  "scripts": {
    "format": "turbo run format --color",
    "format:check": "turbo run format:check --color",
    "build": "turbo run build --color",
    "publish-packages": "turbo run publish-packages --color",
    "publish-packages:next": "turbo run publish-packages:next --color",
    "test:publish-packages": "turbo run test:publish-packages --color",
    "lint": "turbo run lint --color",
    "lint:fix": "turbo run lint:fix --color",
    "int": "prettier --write . --cache --ignore-unknown",
    "int-lint": "eslint --fix .",
    "int:check": "prettier --check .",
    "int-lint:check": "eslint .",
    "generate-docs": "tsx src/generateDocs.ts"
  },
  "keywords": [
    "eslint",
    "shared-configs",
    "prettier",
    "prettier-config",
    "javascript",
    "config",
    "styleguide"
  ],
  "dependencies": {
    "markdown-table": "3.0.4",
    "turbo": "2.4.4"
  },
  "devDependencies": {
    "@igorkowalczyk/eslint-config": "workspace:*",
    "@igorkowalczyk/prettier-config": "workspace:*",
    "@types/node": "22.13.9",
    "eslint": "9.21.0",
    "prettier": "3.5.3",
    "tsx": "4.19.3"
  },
  "packageManager": "pnpm@10.5.2"
}
