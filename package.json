{
  "name": "@igorkowalczyk/sharable-configs",
  "version": "3.2.1",
  "description": "â˜” My sharable ESLint and Prettier configs",
  "author": "Igor Kowalczyk",
  "license": "MIT",
  "repository": "IgorKowalczyk/shared-configs",
  "funding": "https://github.com/sponsors/igorkowalczyk",
  "bugs": "https://github.com/IgorKowalczyk/shared-configs/issues",
  "homepage": "https://github.com/IgorKowalczyk/shared-configs#readme",
  "private": true,
  "type": "module",
  "scripts": {
    "format": "turbo run format",
    "format:check": "turbo run format:check",
    "build": "turbo run build",
    "publish-packages": "turbo run publish-packages",
    "publish-packages:next": "turbo run publish-packages:next",
    "test:publish-packages": "turbo run test:publish-packages",
    "lint": "turbo run lint",
    "lint:fix": "turbo run lint:fix",
    "int": "prettier --write . --cache --ignore-unknown",
    "int-lint": "eslint --fix .",
    "int:check": "prettier --check .",
    "int-lint:check": "eslint .",
    "generate-docs": "tsx src/generateDocs.ts"
  },
  "keywords": [
    "eslint",
    "shared-configs",
    "prettier",
    "prettier-config",
    "javascript",
    "config",
    "styleguide"
  ],
  "dependencies": {
    "markdown-table": "3.0.4",
    "turbo": "2.6.0"
  },
  "devDependencies": {
    "@igorkowalczyk/eslint-config": "workspace:*",
    "@igorkowalczyk/prettier-config": "workspace:*",
    "@types/node": "24.10.0",
    "eslint": "9.39.1",
    "jiti": "2.6.1",
    "prettier": "3.7.4",
    "tsx": "4.20.6"
  },
  "packageManager": "pnpm@10.21.0"
}
